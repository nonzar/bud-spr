doctype html
html(lang='zh-cn')
    head
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        title bud
        link(href='css/bootstrap.min.css', rel='stylesheet')
        link(href="css/global.css",rel="stylesheet")
    body
        .container-fluid.header
            .row
                .col-lg-12
                    include include/header.jade
        .container-fluid
            .row
                .col-lg-2.sidebar-parent
                    include include/mkt-sidebar.jade
                .col-lg-10.contaner
                    .row
                        .col-lg-12.form-search-parent
                            include include/form-search.jade
                    .row
                        .col-lg-12.table-data-parent
                            ul.nav.nav-tabs
                                li.active
                                    a(href="#tab1",data-toggle="tab") SPR状态查询
                                li
                                    a(href="#tab2",data-toggle="tab") 待处理SPR
                            .tab-content
                                .tab-pane.fade.in.active#tab1
                                    .row
                                        .col-lg-12
                                            form.form-inline
                                                .input-group
                                                    input.form-control(type="text",placeholder="请输入")
                                                    span.input-group-btn
                                                        button.btn.btn-default(type="button") 搜索
                                        .col-lg-12
                                            .row
                                                .col-lg-12
                                                    table.table.table-data
                                                        thead
                                                            tr
                                                                th #
                                                                th 促销员id
                                                                th 验证码
                                                                th 大区
                                                                th 城市
                                                                th 城市等级
                                                                th 促销员名称
                                                                th 渠道
                                                                th PTL
                                                                th 状态
                                                                th 操作
                                                        tbody
                                                            tr
                                                                td 1
                                                                td 数据
                                                                td 数据
                                                                td 数据
                                                                td 数据
                                                                td 数据
                                                                td 数据
                                                                td 数据
                                                                td 数据
                                                                td 数据
                                                                td
                                                                    button.btn.btn-default(type="submit") 修改
                                .tab-pane.fade#tab2
                                    .row
                                        .col-lg-12
                                            table.table.table-data
                                                thead
                                                    tr
                                                        th #
                                                        th 促销员id
                                                        th 大区
                                                        th 城市
                                                        th 城市等级
                                                        th 促销员名称
                                                        th 渠道
                                                        th PTL
                                                        th 状态
                                                        th 市场部确认
                                                tbody
                                                    - for(var i=0;i<10;i++)
                                                        tr
                                                            td #{i}
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td
                                                                button.btn.btn-default(type="submit") 确认
                                                                button.btn.btn-default(type="submit") 取消
        script(src='js/jquery.min.js')
        script(src='js/bootstrap.min.js')