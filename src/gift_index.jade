doctype html
html(lang='zh-cn')
    head
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        title 百威
        link(href='css/bootstrap.min.css', rel='stylesheet')
        link(href="css/global.css",rel="stylesheet")
    body
        .container-fluid.header
            .row
                .col-lg-12
                    include include/header.jade
        .container-fluid
            .row
                .col-lg-2.sidebar-parent
                    .nav.nav-pills.nav-stacked(role="tablist")
                        li.active(role="presentation")
                            a(href="#") 礼品管理
                .col-lg-10.contaner
                    .row
                        .col-lg-12.form-search-parent
                            include include/form-search.jade
                    .row
                        .col-lg-12.table-data-parent
                            ul.nav.nav-tabs
                                li.active
                                    a(href="#tab1",data-toggle="tab") 所有兑奖记录
                                li
                                    a(href="#tab2",data-toggle="tab") 高价值兑奖记录
                                li
                                    a(href="#tab3",data-toggle="tab") 库存情况
                            .tab-content
                                .tab-pane.fade.in.active#tab1
                                    .row
                                        .col-lg-12
                                            table.table.table-data
                                                thead
                                                    tr
                                                        th #
                                                        th 记录id
                                                        th openid
                                                        th 产品名称
                                                        th 兑奖时间
                                                        th 姓名
                                                        th 电话
                                                        th 快递单号
                                                        th 地址
                                                tbody
                                                    - for(var i=0;i<10;i++)
                                                        tr
                                                            td #{i}
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                .tab-pane.fade#tab2
                                    .row
                                        .col-lg-12
                                            table.table.table-data
                                                thead
                                                    tr
                                                        th #
                                                        th 记录id
                                                        th openid
                                                        th 产品名称
                                                        th 兑奖时间
                                                        th 姓名
                                                        th 电话
                                                        th 快递单号
                                                        th 地址
                                                tbody
                                                    - for(var i=0;i<10;i++)
                                                        tr
                                                            td #{i}
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                .tab-pane.fade#tab3
                                    .row
                                        .col-lg-12
                                            table.table.table-data
                                                thead
                                                    tr
                                                        th #
                                                        th 奖品TYPE
                                                        th 名称
                                                        th 兑换积分
                                                        th 总量
                                                        th 兑换数量
                                                        th 兑换率
                                                tbody
                                                    - for(var i=0;i<10;i++)
                                                        tr
                                                            td #{i}
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
                                                            td 数据
        script(src='js/jquery.min.js')
        script(src='js/bootstrap.min.js')